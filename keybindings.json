// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "cmd+f6",
        "command": "cordova.run",
        "when": "!inDebugMode"
    },
    {
        "key": "cmd+f5",
        "command": "-cordova.run",
        "when": "!inDebugMode"
    },
    {
        "key": "shift+alt+z",
        "command": "workbench.action.toggleZenMode"
    },
    {
        "key": "ctrl+shift+cmd+f",
        "command": "-workbench.action.toggleZenMode"
    }
]